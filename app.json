{
    "name": "VC PLAYER BOT",
    "logo": "https://telegra.ph//file/47e09c544dcc3838b9f3c.jpg",
    "description": "Open-Source bot to play songs in your Telegram's Group Voice Chat. Powered by @GODBOYX.",
    "keywords": ["music", "voicechat", "telegram"],
    "repository": "https://github.com/GODBOYX/VCPLAYERBOT",
    "stack": "container",
    "env": {
        "SESSION_NAME": {
            "description": "Session string (pyrogram)",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "From @BotFather",
            "required": true
        },
        "BOT_NAME": {
            "description": "Your VCPlayBot Name.",
            "required": false,
            "value": ""
        },
        "API_ID": {
            "description": "From my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "From my.telegram.org/apps",
            "required": true
        },
        "SUDO_USERS": {
            "description": "Full-access users, counted as admin everywhere, seperated by space.",
            "required": true
            "value": "1742906647"
        },
        "DURATION_LIMIT": {
            "description": "In minutes!",
            "required": true,
            "value": "15"
        }
    },
    "buildpacks": [
       {
      "url": "heroku/python"
       }
     ]
  }
